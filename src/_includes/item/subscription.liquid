<section class="item item--{{ item_modifier | default: 'block'}} | h-entry" data-type="subscription">
  <header class="item__header">
    <h3 class="item__title | p-name">
      <a href="{{ item.url }}">{{ item.title }}</a>
    </h3>
  </header>

  <div class="item__main s-linear | p-summary">
    <a href="{{ item.feedUrl }}">{{ item.feedUrl }}</a>
  </div>
</section>
